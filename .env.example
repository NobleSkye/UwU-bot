# Environment variables for the UwU bot
# Copy this file to .env and fill in your actual values

# Your Discord bot token (REQUIRED)
DISCORD_TOKEN=YOUR_DISCORD_BOT_TOKEN_HERE

# Channel ID where the bot should operate (optional, defaults to current value)
CHANNEL_ID=YOUR_CHANNEL_ID_HERE

# Custom emoji ID for reactions (optional, defaults to current value)
CUSTOM_EMOJI_ID=YOUR_CUSTOM_EMOJI_ID_HERE